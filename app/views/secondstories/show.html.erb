<div class="page-header">
  <h3>Secondstory #<%= @secondstory.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Punch line</dt>
      <dd><%= @secondstory.punch_line %></dd>

      <dt>Body</dt>
      <dd><%= @secondstory.body %></dd>

      <dt>Reinterpretation</dt>
      <dd><%= @secondstory.reinterpretation_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/secondstories" class="btn btn-primary">
        Back
      </a>
      <a href="/secondstories/<%= @secondstory.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_secondstory/<%= @secondstory.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
